<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="rectangle">

            <solid android:color="@color/switch_themed_track" />
            <corners android:radius="56dp" />
            <size
                android:width="64dp"
                android:height="28dp" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:shape="rectangle" android:tint="#494949" android:tintMode="src_over">

            <solid android:color="@color/switch_themed_track" />
            <corners android:radius="56dp" />
            <size
                android:width="64dp"
                android:height="28dp" />
        </shape>
    </item>
</selector>